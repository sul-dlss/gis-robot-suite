#!/bin/bash

export ROBOT_LOG_LEVEL=DEBUG
export PATH=./bin:$PATH

druid="$1"

for s in \
  load-raster \
  load-vector \
  load-geoserver \
  reset-geowebcache \
  finish-gis-delivery-workflow \
  reload-geoserver \
  start-accession-workflow \
  ; do
  bundle exec run_robot dor:gisDeliveryWF:$s -d $druid
done
